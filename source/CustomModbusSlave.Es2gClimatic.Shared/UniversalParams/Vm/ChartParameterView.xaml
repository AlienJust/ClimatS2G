	<UserControl.Resources>
		<ResourceDictionary Source="AggregateAndChartParamResources.xaml"/>
	</UserControl.Resources>
	<CheckBox IsChecked="{Binding Path=IsChecked, Mode=TwoWay}" HorizontalContentAlignment="Stretch">
