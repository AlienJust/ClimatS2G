<UserControl x:Class="CustomModbusSlave.MicroclimatEs2gApp.MukAirExhauster.View.AirExhausterDataView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:view="clr-namespace:CustomModbusSlave.MicroclimatEs2gApp.MukAirExhauster.View"
             xmlns:setParameters="clr-namespace:CustomModbusSlave.MicroclimatEs2gApp.MukAirExhauster.SetParameters"
             xmlns:es2GClimatic="clr-namespace:CustomModbusSlave.Es2gClimatic;assembly=CustomModbusSlave.Es2gClimatic"
             mc:Ignorable="d" 
             d:DesignHeight="312" d:DesignWidth="300">
	<StackPanel Orientation="Vertical">
		<view:AirExhausterReply03DataView DataContext="{Binding Path=Reply03Telemetry, Mode=OneWay}"/>
		<es2GClimatic:AnyCommandPartView DataContext="{Binding Path=Reply03TelemetryText, Mode=OneTime}"/>
		<TextBlock Text="Уставка параметров:" FontWeight="Bold" TextDecorations="Underline"/>
		<setParameters:MukAirExhausterSetParamsView DataContext="{Binding Path=MukAirExhausterSetParamsVm, Mode=OneTime}"/>
	</StackPanel>
</UserControl>
