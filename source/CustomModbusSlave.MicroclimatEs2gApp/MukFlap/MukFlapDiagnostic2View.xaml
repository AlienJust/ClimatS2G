<UserControl x:Class="CustomModbusSlave.MicroclimatEs2gApp.MukFlap.MukFlapDiagnostic2View"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CustomModbusSlave.MicroclimatEs2gApp.MukFlap"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
	<StackPanel>
		<CheckBox Content="ОС: заслонка не встает в крайние положения" IsChecked="{Binding Path=OsShowsFlapDoesNotReachLimitPositions, Mode=OneTime}"/>
		<CheckBox Content="ОС: заслонка не проходит 50% диапазона" IsChecked="{Binding Path=OsShowsFlapDoesNotReach50Percent, Mode=OneTime}"/>
	</StackPanel>
</UserControl>
